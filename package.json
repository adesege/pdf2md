{
  "name": "@opendocsg/pdf2md",
  "version": "0.2.1",
  "description": "A PDF to Markdown Converter",
  "main": "lib/pdf2md.ts",
  "type": "module",
  "bin": {
    "pdf2md": "lib/pdf2md-cli.ts"
  },
  "files": [
    "/lib",
    "/test",
    "/types"
  ],
  "scripts": {
